{
    "name": "SlackRTL",
    "description": "Make Slack RTL compatible",
    "version": "1.0",
    "permissions": [
        "tabs","<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "MakeRTL"
    },
    "manifest_version": 2
}
